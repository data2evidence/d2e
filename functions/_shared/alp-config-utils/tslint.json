{
    "extends": "tslint:recommended",
    "env": {
        "node": true
    },
    "rules": {
        "prefer-for-of": false,
        "no-shadowed-variable": false,
        "jsdoc-format": false,
        "no-console": false,
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            160
        ],
        "interface-name": [
            false
        ],
        "object-literal-sort-keys": false
    }
}
