{
    "name": "analytics",
    "version": "2.0.0",
    "description": "Patient Analytics",
    "private": true,
    "scripts": {
        "testci": "jest --runInBand",
        "testcicc": "jest --runInBand --coverage --forceExit --ci --testResultsProcessor=\"jest-junit\" --detectOpenHandles",
        "lint": "tslint --config ./tslint.json --project ./tsconfig.json",
        "start": "node target/src/main.js",
        "compile": "tsc",
        "start:dev": "tsnd --inspect=0.0.0.0:9222 --respawn ./src/main.ts",
        "start:docker": "yarn convertenv && . ./generated-env.sh && if [ \"$LOCAL_DEBUG\" = \"true\" ] ; then yarn start:dev; else yarn start; fi",
        "buildvue": "grunt css && cd ../vue && npm run build",
        "convertenv": "cd envConverter && yarn generate:envs -s alp-minerva-analytics-svc"
    },
    "keywords": [],
    "author": "",
    "engines": {
        "node": ">=18.20.4"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:alp/alp-clinical-research.git"
    },
    "dependencies": {
        "@alp/alp-base-utils": "1.1.9",
        "@alp/alp-config-utils": "1.0.20",
        "@alp/alp-metadata": "1.0.2",
        "@alp/swagger-node-runner": "0.7.4-1",
        "@godaddy/terminus": "^4.4.1",
        "@sap/xsenv": "^3.3.2",
        "async": "3.2.4",
        "csv-write-stream": "^2.0.0",
        "dotenv": "10.0.0",
        "duckdb-async": "^1.1.1",
        "express": "^4.18",
        "express-validator": "6.6.1",
        "hdb": "^0.19.3",
        "helmet": "4.6.0",
        "jsonwebtoken": "^9.0.0",
        "jstat": "1.9.3",
        "jszip": "3.10.1",
        "liquibase": "^0.0.0",
        "minio": "7.0.30",
        "pako": "1.0.11",
        "parquetjs": "^0.11.2",
        "pg": "^8.4.2",
        "ts-node-dev": "^2.0.0",
        "tslint": "5.12.1",
        "typescript": "5.5.4",
        "typescript-string-operations": "^1.5.0",
        "underscore": "^1.12.1",
        "winston": "^3.7.2",
        "zod": "3.20.6"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/fs-extra": "4.0.11",
        "@types/jest": "^29.5.11",
        "@types/minio": "7.0.13",
        "@types/node": "^18",
        "fs-extra": "10.0.0",
        "if-async": "^3.7.4",
        "jest": "^29.7.0",
        "jest-junit": "^5.2.0",
        "nyc": "15.1.0",
        "prettier": "2.3.1",
        "supertest": "^6.1.3",
        "ts-jest": "29.1.2",
        "ts-node-dev": "^2.0.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "unzipper": "^0.9.11"
    },
    "nyc": {
        "lines": 55,
        "statements": 55,
        "functions": 55,
        "branches": 40,
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings",
            "target"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "cobertura",
            "text-summary"
        ],
        "check-coverage": true,
        "all": true
    },
    "jest": {
        "testURL": "http://localhost",
        "verbose": true,
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "roots": [
            "spec",
            "src"
        ],
        "testRegex": "_(test|integrationtest).ts$",
        "testPathIgnorePatterns": [
            ".*kaplan_meier_integrationtest.ts$",
            ".*query_object_dbapi_integrationtest.ts$",
            ".*domain_values_service_integrationtest.ts$",
            ".*aggquery_external_custom_table_integrationtest.ts$"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/ps/"
        ],
        "coverageReporters": [
            "cobertura",
            "html",
            "text"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "spec",
            "target",
            "typings"
        ],
        "testTimeout": 600000
    }
}