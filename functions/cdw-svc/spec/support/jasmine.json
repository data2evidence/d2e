{
    "spec_dir": "spec",
    "spec_files": [
        "*_integrationtest.ts",
        "*/*_integrationtest.ts",
        "*/*/*_integrationtest.ts",
        "*_test.ts",
        "*/*_test.ts",
        "*/*/*_test.ts"
    ],
    "reporters": [
        {
          "name": "jasmine-spec-reporter#SpecReporter",
          "options": {
            "displayStacktrace": "all"
          }
        },
        {
            "name": "jasmine-reporters#JUnitXmlReporter"
          }
      ],
    "stopSpecOnExpectationFailure": false,
    "random": false
}