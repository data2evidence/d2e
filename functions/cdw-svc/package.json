{
  "name": "cdw-config",
  "version": "3.0.0-dev",
  "description": "Medical Research Insights",
  "private": true,
  "main": "./target/src/qe.js",
  "typings": "./target/src/qe.d.ts",
  "engines": {
    "node": ">=18.20.4"
  },
  "scripts": {
    "test": "jest --runInBand",
    "test:ci": "jest --verbose --runInBand --detectOpenHandles --coverage --forceExit --reporters=\"jest-junit\"",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json",
    "start": "node target/src/main.js",
    "start:docker": "yarn convertenv && . ./generated-env.sh && if [ \"$LOCAL_DEBUG\" = \"true\" ] ; then yarn dev; else yarn start; fi",
    "compile": "tsc",
    "localdevjs": "npm run compile && npm start",
    "dev2": "TS_NODE_FILES=true TS_NODE_PROJECT=./tsconfig.json nodemon --watch src --watch src_ui -e js,ts,json --exec \"ts-node src/main.ts\"",
    "dev": "tsnd --inspect=0.0.0.0:9206 --respawn ./src/main.ts",
    "postgres:start": "docker-compose down && docker-compose up",
    "convertenv": "cd envConverter && yarn generate:envs -s alp-minerva-cdw-svc"
  },
  "keywords": [],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:alp/alp-clinical-research.git"
  },
  "dependencies": {
    "@alp/alp-base-utils": "1.1.9",
    "@alp/alp-config-utils": "1.0.20",
    "@sap/xsenv": "^3.3.2",
    "dotenv": "16.0.1",
    "hdb": "0.19.3",
    "duckdb-async": "^1.1.1",
    "express": "^4.18",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.5.4",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.19.31",
    "fs-extra": "^10.1.0",
    "if-async": "^3.7.4",
    "jest": "^29.7.0",
    "jest-junit": "^5.2.0",
    "nodemon": "^2.0.13",
    "sinon": "1.17.3",
    "ts-jest": "29.1.2",
    "tslint": "^6.1.3"
  },
  "resolutions": {},
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "_(test|integrationtest).ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 32.32,
        "statements": 32.66,
        "functions": 31.85,
        "branches": 24.36
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "spec",
      "target",
      "typings"
    ],
    "moduleDirectories": [
      "node_modules",
      "spec",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "text"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}