{
  "name": "genomics-svc",
  "description": "Genomics node service",
  "main": "dist/server.js",
  "dependencies": {
    "@sap/audit-logging": "2.2.4",
    "@sap/hdbext": "4.7.5",
    "@sap/logging": "3.4.0",
    "@sap/xsenv": "1.2.9",
    "@sap/xssec": "2.1.15",
    "body-parser": "1.18.3",
    "express": "4.16.3",
    "node-cache": "4.2.0",
    "passport": "0.4.0"
  },
  "devDependencies": {
    "grunt": "1.0.3",
    "grunt-cli": "1.3.1",
    "grunt-contrib-clean": "2.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-watch": "1.1.0",
    "grunt-peg": "2.0.1",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "3.2.0",
    "jasmine-console-reporter": "^3.1.0",
