VIEW "hc.hph.cdw.db.models::DWTargetDocumentStore" ( "Target", "Type" ) AS ((SELECT '"'||"SCHEMA_NAME"||'"."'||"TABLE_NAME"||'"' as "Target", 'Table' as "Type" FROM "hc::TABLES" where "SCHEMA_NAME" IN (Select CURRENT_SCHEMA from "hc::DUMMY")) UNION (SELECT '"'||"SCHEMA_NAME"||'"."'||"VIEW_NAME"||'"' as "Target", 'View' as "Type" FROM "hc::VIEWS" where "SCHEMA_NAME"IN (Select CURRENT_SCHEMA from "hc::DUMMY"))) WITH READ ONLY