FUNCTION "hc.hph.plugins.textProcessing.pathology.postProcessing.lib.table_functions::uncollapsible_subentities" ( ) 
	RETURNS "hc.hph.plugins.textProcessing.pathology.postProcessing::Types.uncollapsible_subentities"
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN

RETURN SELECT
	"DWID",
	"TA_TOKEN",
	"TA_TYPE",
	"TA_OFFSET",
	"TA_PARENT",
	"TA_NORMALIZED"
FROM "hc.hph.plugins.textProcessing.pathology.postProcessing.lib.table_functions::ta"()
WHERE "TA_TYPE" IN (
	'Observation_CancerGrade_Value', 'Observation_CancerSize_Value',
	'Observation_CancerStage_Value_Summarized', 'Observation_CancerStage_Value_TNMComplete', 'Observation_CancerStage_Component_T', 'Observation_CancerStage_Component_N', 'Observation_CancerStage_Component_M', 'DIGIT',
	'Observation_CancerLymphNodes_Value_Denominator', 'Observation_CancerLymphNodes_Value_Numerator_Pos', 'Observation_CancerLymphNodes_Value_Numerator_Neg', 'Observation_CancerLymphNodes_Component_Laterality', 'Observation_CancerLymphNodes_BodySite', 'Observation_CancerLymphNodes_Interpretation','Observation_CancerLymphNodes_Component_Extension','Observation_CancerLymphNodes_Component_Size',
	'IND', 'IE',
	'Observation_CancerBiomarker_Method', 'Observation_CancerBiomarker_Component_DNARegion', 'Observation_CancerBiomarker_Value');

END;
