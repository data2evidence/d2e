PROCEDURE "hc.hph.plugins.gff3.db.procedures::ClearStagingTables" (
		IN iAuditID BIGINT
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY DEFINER
	AS
BEGIN
	CALL "hc.hph.di.procedures::clean_logs" ( iAuditID );
	DELETE FROM "hc.hph.plugins.gff3.db.models::Staging.Chromosomes" WHERE "DWAuditID" = :iAuditID;
	DELETE FROM "hc.hph.plugins.gff3.db.models::Staging.Features" WHERE "DWAuditID" = :iAuditID;
END;
