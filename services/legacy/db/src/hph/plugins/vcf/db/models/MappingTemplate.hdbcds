namespace hc.hph.plugins.vcf.db.models;

context MappingTemplate
{

	entity Mappings
	{
		key TemplateName : String(100);
		key UserName : String(255);
		Description: String(255);
		Content : LargeString null; 
		Date: UTCTimestamp;
	} technical configuration {
 	   column store;
 	   partition by keeping existing layout;
           migration disabled;
  	};
};