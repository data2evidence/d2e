PROCEDURE "hc.hph.plugins.fasta.db.procedures::getLastStatus" (
IN auditID BIGINT,
OUT status INT 
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
DECLARE EXIT HANDLER FOR SQL_ERROR_CODE 1299
BEGIN
             	SELECT 0 INTO status FROM "hc::DUMMY";
END;
select "Status" into status from "hc.hph.plugins.fasta.db.models::FastaAdapterRunStatus.StatusValue" where "AuditLogID" = :auditID;

END;