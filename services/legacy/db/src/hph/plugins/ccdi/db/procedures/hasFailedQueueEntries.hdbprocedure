PROCEDURE "hc.hph.plugins.ccdi.db.procedures::hasFailedQueueEntries" (
		in auditLogId	BIGINT,
		out hasFailedQueueEntries	TINYINT
   ) 
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER

   READS SQL DATA
   AS   
BEGIN  

SELECT (CASE WHEN (COUNT(*) > 0) THEN 1 ELSE 0 END) INTO hasFailedQueueEntries 
FROM "hc.hph.plugins.ccdi.db.models::Tasks.Queue" WHERE 
 "auditLogId" = :auditLogId  
 AND "STATUS" ='Failed';

END;