PROCEDURE "hc.hph.plugins.ccdi.db.procedures::deleteQueueEntries" (
		in auditLogId	BIGINT
   ) 
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER

   --READS SQL DATA AS
   AS   
BEGIN  

DELETE FROM "hc.hph.plugins.ccdi.db.models::Tasks.Queue" WHERE "auditLogId" = :auditLogId;

DELETE FROM "hc.hph.plugins.ccdi.db.models::Tasks.NotedScheduleId" WHERE "auditLogId" = :auditLogId;

END;