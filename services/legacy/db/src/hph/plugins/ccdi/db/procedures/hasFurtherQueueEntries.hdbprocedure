PROCEDURE "hc.hph.plugins.ccdi.db.procedures::hasFurtherQueueEntries" (
		in auditLogId	BIGINT,
		out hasFurtherQueueEntries	TINYINT
   ) 
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER

   READS SQL DATA
   AS   
BEGIN  

SELECT (CASE WHEN (COUNT(*) > 0) THEN 1 ELSE 0 END) INTO hasFurtherQueueEntries 
FROM "hc.hph.plugins.ccdi.db.models::Tasks.Queue" WHERE 
 "auditLogId" = :auditLogId  
 AND "STATUS"='Queued';

END;