PROCEDURE "hc.hph.plugins.bed.db.procedures::setStatus" (  AuditLogID BIGINT, Status integer) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
if :Status = 1 then
	insert into "hc.hph.plugins.bed.db.models::AdapterRunStatus.StatusValue" ("AuditLogID","Status") values (:AuditLogID, :Status);
else
	update "hc.hph.plugins.bed.db.models::AdapterRunStatus.StatusValue" set "Status" = :Status where "AuditLogID" = :AuditLogID;
end if;
END;