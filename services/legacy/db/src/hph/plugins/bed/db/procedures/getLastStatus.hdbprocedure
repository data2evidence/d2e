PROCEDURE "hc.hph.plugins.bed.db.procedures::getLastStatus" ( IN auditID BIGINT,
OUT status INT 
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
declare exit handler for SQL_ERROR_CODE 1299
begin
select 0 into status from "hc::DUMMY";
end;
select "Status" into status from "hc.hph.plugins.bed.db.models::AdapterRunStatus.StatusValue" where "AuditLogID" = :auditID;

END;