namespace hc.hph.plugins.ped.db.models;
using hc.hph.genomics.db.models::Type as Type;
context PedAdapterRunStatus {

    entity StatusValue
    {
    	key AuditLogID : Type.DWDocID;
        Status : Integer;

    } technical configuration {
        column store;
    }; 

    entity TaskExecution
    {
    	key AuditLogID: Type.DWAuditID;
    	TaskExecutionID: Type.DWAuditID;
    	TaskName: String(255);
    } technical configuration {
     column store;
    };

};