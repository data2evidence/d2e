PROCEDURE "hc.hph.plugins.ped.db.procedures::getLastStatus" ( 
IN auditID BIGINT,
OUT STATUS INT) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
/***************************** 
	Write your procedure logic 
 *****************************/
declare exit handler for SQL_ERROR_CODE 1299
begin
select 0 into STATUS from "hc::DUMMY";
end;
select "Status" into STATUS from "hc.hph.plugins.ped.db.models::PedAdapterRunStatus.StatusValue" where "AuditLogID" = :auditID;
  
END;
