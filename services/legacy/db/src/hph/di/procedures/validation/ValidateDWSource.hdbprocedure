PROCEDURE "hc.hph.di.procedures.validation::ValidateDWSource" ( IN source NVARCHAR(255), OUT status TINYINT )
LANGUAGE SQLSCRIPT SQL SECURITY INVOKER READS SQL DATA AS
BEGIN
    DECLARE count INTEGER := 0;
    SELECT COUNT(*) INTO count FROM "hc.hph.di.model::DataIntegration.DISource" WHERE "SourceID" = :source;
    status := 0;
    IF :count = 1 THEN
        status := 1;
    END IF;
END