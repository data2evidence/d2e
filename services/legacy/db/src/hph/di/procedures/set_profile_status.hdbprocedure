PROCEDURE "hc.hph.di.procedures::set_profile_status" (in profile_id BIGINT,IN Status NVARCHAR(16)) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
	update "hc.hph.di.model::DataIntegration.JobProfile" profile 
		set profile."Status" = :Status,
			profile."ModifiedAt"= CURRENT_UTCTIMESTAMP,
			profile."ModifiedBy"=(Select SESSION_CONTEXT('XS_APPLICATIONUSER') From "hc::DUMMY")		
		
		FROM "hc.hph.di.model::DataIntegration.JobProfile" profile
		where profile."ProfileID" = :profile_id;
END;
