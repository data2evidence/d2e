PROCEDURE "hc.hph.eula.procedures::EXISTS_EULA" ( 
		in in_eula_id NVARCHAR(32),
		in in_eula_idname NVARCHAR(256),
		out out_found INT ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	READS SQL DATA AS
BEGIN
   declare found INT := 0;
 
   select count(*) into found
   		from "hc.hph.eula.model::Eula_Model.Eula"
   		where "ID" = :in_eula_id OR
   			  "IdName" = :in_eula_idname;
  
   if :found = 0 then 
   	 out_found := 0;
   else 
   	 out_found := 1;
   end if; 
END;  
