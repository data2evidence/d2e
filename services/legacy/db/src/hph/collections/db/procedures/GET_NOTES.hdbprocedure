PROCEDURE "hc.hph.collections.db.procedures::GET_NOTES" ( 
    IN  CollectionId nvarchar(256),
    IN PatientId nvarchar(256),
    OUT Notes "hc.hph.collections.db.models::TT_OUT_NOTES") 
                
LANGUAGE SQLSCRIPT SQL SECURITY DEFINER AS
BEGIN
    
    Notes = SELECT "Type","Id","Text","Content","CreatedBy","CreatedAt" FROM 
    "hc.hph.collections.db.models::CollectionModel.Comment"
    where "Collection.Id"=CollectionId
    and "Item.Id" = PatientId;   
    
END;
