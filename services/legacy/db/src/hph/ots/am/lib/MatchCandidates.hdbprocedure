procedure "hc.hph.ots.am.lib::MatchCandidates"(
		in  iv_profile_name nvarchar(100),
		in  it_candidates   "hc.hph.ots.am::Types.Candidate",
		out ot_matches      "hc.hph.ots.am::Types.Match"
	)
	language sqlscript
	sql security definer
	as
begin
    ot_matches = 
    select
        "CandidateID",
        "ComponentID",
        "ComponentTermText",
        "VocabularyID",
        "Code",
        null as "TermText",
        0.0 as "Confidence"
    from :it_candidates;
end
;
