PROCEDURE "hc.mri.pa.db::MRIUserDisplay" ( 
   in UserName NVARCHAR(40), 
   out UserInfo "hc.mri.pa.db::MRIEntities.Bookmarks"
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	READS SQL DATA AS
BEGIN

    UserInfo = select * FROM "hc.mri.pa.db::MRIEntities.Bookmarks" WHERE "UserName" = UserName;
  
END;
