{
	"resourceType": "Permission",
	"id": "HCIMPatientGroupPermission",
	"group": "HCIMPatientGroup",
	"requiredAttributes": [
		"attr.socialSecurityNumber",
		"attr.identifierSystem"
	],
	"restPermission": [
		{
			"type": "compartement",
			"definition": {
				"reference": "http://hl7.org/fhir/StructureDefinition/Patient"
			},
			"grant": [
				"read"
			]
		}
	],
	"instancePermission": [
		{
			"type": "compartement",
			"definition": {
				"reference": "http://hl7.org/fhir/StructureDefinition/Patient"
			},
			"grant": [
				"read"
			],
			"constraintType": "elementMap",
			"element": "Patient.identifier.value",
			"value": "$attr.identifierSystem|$attr.socialSecurityNumber"
		}
	]
}