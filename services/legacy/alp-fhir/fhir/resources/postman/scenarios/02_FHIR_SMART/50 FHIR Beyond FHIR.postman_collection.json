	"item": [
		{
			"name": "Query for Parts of Observations",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "3c49cf9d-8ec1-4ce6-a4d6-ec9067dd2eb8",
						"type": "text/javascript",
						"exec": [
							"pm.test(\"Status code is 200 OK\", function () {",
							"    pm.response.to.have.status(200);",
							"    pm.response.to.have.status(\"OK\");",
							"});",
							"",
							"pm.test(\"Content-Type is present\", function () {",
							"    pm.response.to.have.header(\"Content-Type\");",
							"    pm.expect(postman.getResponseHeader(\"Content-Type\")).to.include(\"application/fhir+json\");",
							"});",
							"",
							"pm.test(\"Validate Response Body\", function () {",
							"    var response = pm.response.json();",
							"    for(var resource of response.entry) {",
							"        pm.expect(resource.resource.display).to.eql(\"Body Weight\");",
							"    }",
							"});"
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					},
					{
						"key": "Accept",
						"value": "application/fhir+json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\"type\":\"query\",\n\t\"from\":{\n\t\t\"type\":\"from\",\"definition\":\"Observation\"},\n\t\t\"name\":\"TestObservationQuery\",\n\t\t\"filter\":{\"type\":\"binaryexpression\",\n\t\t\t\"left\":{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.code.coding.code\"},\n\t\t\t\"right\":{\"type\":\"stringexpression\",\"value\":\"29463-7\"},\n\t\t\t\"operation\":\"=\"},\n\t\t\"out\":[\n\t\t\t{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.valueQuantity.value\"},\n\t\t\t{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.code.coding.display\"}\n\t\t]\n}"
				},
				"url": {
					"raw": "{{protocol}}://{{host}}:{{port}}/fhir/{{endpoint}}/query",
					"protocol": "{{protocol}}",
					"host": [
						"{{host}}"
					],
					"port": "{{port}}",
					"path": [
						"fhir",
						"{{endpoint}}",
						"query"
					]
				}
			},
			"response": []
		},
