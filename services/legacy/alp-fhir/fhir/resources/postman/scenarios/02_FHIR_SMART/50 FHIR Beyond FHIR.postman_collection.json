		{
			"name": "Get Per Gender and VitalSign Average and Max Value",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "6c9c6a5a-f712-4582-9f7e-7ed3033a8e6c",
						"type": "text/javascript",
						"exec": [
							"pm.test(\"Status code is 200 OK\", function () {",
							"    pm.response.to.have.status(200);",
							"    pm.response.to.have.status(\"OK\");",
							"});",
							"",
							"pm.test(\"Content-Type is present\", function () {",
							"    pm.response.to.have.header(\"Content-Type\");",
							"    pm.expect(postman.getResponseHeader(\"Content-Type\")).to.include(\"application/fhir+json\");",
							"});",
							"",
							"pm.test(\"Validate Response Body\", function () {",
							"    var response = pm.response.json();",
							"    pm.expect(response.entry.length).to.be.below(101);",
							"});"
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					},
					{
						"key": "Accept",
						"value": "application/fhir+json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\"type\":\"query\",\r\n\t\"from\":{\"type\":\"from\",\"definition\":\"Patient\"},\r\n\t\"name\":\"TestObservationQuery\",\r\n\t\"out\":[\r\n\t\t\t{\"type\":\"resultelement\",\"definition\":\"Patient\",\"path\":\"Patient.gender\"},\r\n\t\t\t{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.code.coding.display\"},\r\n\t\t\t{\"label\":\"average\",\"type\":\"unaryfunction\",\"function\":\"avg\",\"expression\":{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.valueQuantity.value\"}},\r\n\t\t\t{\"label\":\"max\",\"type\":\"unaryfunction\",\"function\":\"max\",\"expression\":{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.valueQuantity.value\"}}\r\n\t],\r\n\t\"joins\":[\r\n\t\t{\"type\":\"join\",\"definition\":\"Observation\",\"link\":{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.subject.reference\"}}],\r\n\t\"groups\":[{\"type\":\"resultelement\",\"definition\":\"Patient\",\"path\":\"Patient.gender\"},\r\n\t\t\t{\"type\":\"resultelement\",\"definition\":\"Observation\",\"path\":\"Observation.code.coding.display\"}],\r\n\t\"limit\":{ \"type\":\"limit\",\"limit\":100}\r\n}"
				},
