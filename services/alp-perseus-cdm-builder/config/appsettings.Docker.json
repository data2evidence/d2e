{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "CorsUrl": "http://localhost:9000",
  
    "Postgre": "Driver={PostgreSQL UNICODE};Server={server};Port={port};Database={database};Uid={username};Pwd={password};sslmode=require;UseDeclareFetch=1;",
    "PostgreSQL": "Driver={PostgreSQL UNICODE};Server={server};Port={port};Database={database};Uid={username};Pwd={password};sslmode=require;UseDeclareFetch=1;",
    "mysql": "DRIVER={MySQL ODBC 8.0 UNICODE Driver};SERVER={server};Port={port};DATABASE={database};USER={username};PASSWORD={password};",
    "mssql": "Driver={ODBC Driver 17 for SQL Server};Server={server};Port={port};Database={database};Uid={username};Pwd={password};",
    "azure": "Driver={ODBC Driver 17 for SQL Server};Server={server};Port={port};Database={database};Uid={username};Pwd={password};",
    "databricks": "Driver={Simba Spark ODBC Driver};Host={server};Port={port};UseNativeQuery=1;HTTPPath={username};ThriftTransport=2;SSL=1;AuthMech=3;UID=token;PWD={password}",
  
    "AllowedHosts": "*",
    "UseAureFunctions": "False",
  
    "VaultURI": "",
  
    "BuilderSecretKey": "00000000000000000000000000000000",
  
    "SharedDbHost": "{PG__HOST}",
