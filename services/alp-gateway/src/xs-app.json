{ 
  "routes":
  [
    {
      "source":"^/portal/static/css/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/portal/static/css/"
    },
    {
      "source":"^/portal/static/js/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/portal/static/js/"
    },
    {
      "source":"^/portal/static/media/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/portal/static/media/"
    },
    {
      "source":"^/portal/icons/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/portal/icons/"
    },
    {
      "source":"^/portal/assets/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/portal/assets/"
