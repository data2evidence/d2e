    {
      "source":"^/starboard-notebook-base/(.*)",
      "target": "$1",
      "targetPath":"/ui-files/starboard-notebook-base/"
    },
    {
      "source":"^/hc/hph/core/ui/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/utils/core/"
    },
    {
      "source": "^/hc/hph/config/global/ui/(.*)$",
      "target": "$1",
