    {
      "source": "^/hc/mri/pa/config/i18n/(.*)$",
      "target": "$1",
      "targetPath": "/ui-files/mri-ui5/PatientAnalyticsConfig/ui5/i18n/"
    },
    {
      "source": "^/ui/core/(.*)$",
      "target": "$1",
      "targetPath": "/ui-files/ui5/resources/sap/ui/core/"
    },
    {
      "source": "^/ui/m/(.*)$",
      "target": "$1",
      "targetPath": "/ui-files/ui5/resources/sap/m/"
    },
    {
      "source": "^/hc/hph/patient/app/ui/(.*)$",
      "target": "$1",
      "targetPath": "/ui-files/mri-ui5/chp-ps-ui/"
