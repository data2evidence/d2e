    {
      "source": "^/hc/hph/cdw/config/i18n/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/CDM/ui5/i18n/"
    },
    {
      "source": "^/hc/hph/cdw/config/ui/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/CDM/ui5/"
    }, 
    {  
      "source":"^/hc/hph/patient/config/ui/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/PatientSummaryConfig/ui5/"
    },
    {
      "source":"^/hc/hph/patient/shared/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/PatientSummaryConfig/ui5/shared/"
    },
    {  
      "source":"^/hc/hph/patient/config/i18n/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/PatientSummaryConfig/ui5/i18n/"
    },
    {
      "source":"^/ui/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/ui5/resources/"
    },
    {
      "source":"^/terminology",
      "destination":"alp-terminology-svc"
    },
    {
      "source":"^/alp-sqleditor",
      "destination":"sqleditor"
    },
    {
      "source":"^/alp-ai",
      "destination":"alp-ai-svc"
    },
    {
      "source":"^/meilisearch-svc",
      "destination":"meilisearch-svc"
    },
    {
      "source": "^/pa-config-svc/(.*)$",
      "destination":"pa-config"
    },
    {
      "source": "^/hc/hph/cdw/(.*)$",
      "destination": "cdw"
    },
    {
      "source": "^/hc/hph/config/services/(.*)$",
      "destination": "cdw"
    },
    {
      "source": "^/ps-config-svc/(.*)$",
      "destination": "ps-config"
    }
  ]
}
