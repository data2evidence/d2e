    {
      "source": "^/hc/mri/pa/ui/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/mri-pa-ui/mri/"
    },
    {
      "source": "^/hc/hph/genomics/ui/(.*)$",
      "target": "$1",
      "targetPath":"/ui-files/mri-ui5/genomics-ui/ui/"
