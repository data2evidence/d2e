{
  "name": "alp-db-svc",
  "version": "1.0.0",
  "description": "Manage database objects in an automated way",
  "main": "target/app.js",
  "scripts": {
    "start": "node target/app.js",
    "start:dev": "tsnd --inspect=0.0.0.0:9221 --respawn ./src/app.ts",
    "dev": "tsnd --respawn --transpile-only src/app.ts",
    "compile": "tsc",
    "buildstart": "tsc && node target/app.js",
    "enable:local-test:setup": "rm -rf tmp && cd .. && mkdir -p tmp && cp -prnl alp-db-svc tmp && cp -prnl ../alp-data-node-override/services/alp-db-svc tmp && mv tmp alp-db-svc && cd -",
    "start:docker": "yarn convertenv && . ./generated-env.sh && if [ \"$LOCAL_DEBUG\" == \"true\" ] ; then yarn start:dev; else yarn start; fi",
    "test": "NODE_ENV=test jest --runInBand src/__tests__/integrationTest.spec.ts",
    "test:local": "npm run enable:local-test:setup && NODE_ENV=test jest --watch --runInBand tmp/alp-db-svc/src/__tests__/integrationTest.spec.ts",
    "pgtest": "NODE_ENV=test jest --runInBand --verbose src/__tests__/postgresIntegrationTest.spec.ts",
    "pgtest:local": "npm run enable:local-test:setup && NODE_ENV=test jest --runInBand --watch --verbose tmp/alp-db-svc/src/__tests__/postgresIntegrationTest.spec.ts",
    "cdm-install-script": "NODE_ENV=script node target/script.js",
    "cdm-install-staging-area-script": "tsc && NODE_ENV=script node target/script_staging_area.js",
    "convertenv": "cd envConverter && yarn generate:envs -s alp-minerva-db-mgmt-svc"
  },
  "keywords": [],
  "author": "D4L",
  "license": "ISC",
  "devDependencies": {
    "@types/async": "3.2.3",
    "@types/express": "4.17.13",
    "@types/jest": "^26.0.23",
    "@types/minio": "7.0.13",
    "@types/node": "^18",
    "@types/parquetjs": "^0.10.3",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "supertest": "^6.1.3",
    "ts-jest": "25.5.1",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@alp/alp-base-utils": "1.1.9",
    "@godaddy/terminus": "^4.4.1",
    "async": "3.2.4",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-validator": "6.6.1",
    "hdb": "^0.19.3",
    "liquibase": "^0.0.0",
    "minio": "7.0.30",
    "parquetjs": "^0.11.2",
    "pg": "^8.7.1",
    "tslint": "5.12.1",
    "typescript": "4.4.4",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.7.2"
  },
  "resolutions": {
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "ts-jest": "25.5.1"
  }
}